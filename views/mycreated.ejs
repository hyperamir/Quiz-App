<!DOCTYPE html>
<html>

<head>
  <link rel="stylesheet" href="../styles/create.css" type="text/css" />
  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
</head>

<body>
  <%- include('partials/_header') %>


    <h2> My list of Quizzes </h2>


    <form action= "/myquizzes/:id/:quiz" method="POST">
      <% for (const result of results) { %>

        <label for="fname">
          <%= result.name%>
        </label><br>

        <label for="quiz1"><a href="http://localhost:8080/myquizzes/<%= result.user_id%>/<%= result.id %>">
            Change who can view your quiz
          </a></label><br><br>
        </label><br>
        <% } %>


    </form>

</body>

</html>
